{
  "name": "dev.chrisstone.web",
  "displayName": "Chris Stone",
  "version": "1.0.0",
  "description": "Web app portfolio for Chris Stone.",
  "author": "Chris Stone",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --browsers=ChromeHeadless",
    "test:i": "ng test --browsers=Chrome",
    "deploy": "ng deploy",
    "lint": "ng lint",
    "e2e": "cypress run",
    "e2e:i": "cypress open",
    "favicon": "real-favicon check-for-update --fail-on-update faviconData.json",
    "favicon:generate": "real-favicon generate faviconDescription.json faviconData.json src",
    "scully": "scully --project personal-website",
    "scully:build": "npm run build && npm run scully --scanRoutes",
    "scully:serve": "npm run scully:build && scully serve --",
    "scully:build:verbose": "npm run build --verbose && npm run scully:build --logSeverity=normal"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.1",
    "@angular/common": "^14.2.1",
    "@angular/compiler": "^14.2.1",
    "@angular/core": "^14.2.1",
    "@angular/forms": "^14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "@angular/router": "^14.2.1",
    "@scullyio/init": "^2.1.32",
    "@scullyio/ng-lib": "^2.0.2",
    "@scullyio/scully": "^2.1.32",
    "@scullyio/scully-plugin-puppeteer": "^2.1.32",
    "rxjs": "^7.5.5",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular-eslint/builder": "^14.0.4",
    "@angular-eslint/eslint-plugin": "^14.0.4",
    "@angular-eslint/eslint-plugin-template": "^14.0.4",
    "@angular-eslint/schematics": "^14.0.4",
    "@angular-eslint/template-parser": "^14.0.4",
    "@angular/cli": "^14.2.2",
    "@angular/compiler-cli": "^14.2.1",
    "@angular/language-service": "^14.2.1",
    "@types/jasmine": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "angular-cli-ghpages": "^1.0.0",
    "cli-real-favicon": "^0.0.8",
    "cypress": "^10.7.0",
    "eslint": "^8.18.0",
    "jasmine-core": "^4.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.0.1",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-teamcity-reporter": "^1.1.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.8.3"
  }
}
